<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>{{title}}</title>
</head>
<body>
	<h2>Per assignee per status issue count</h2>
	<table border="5">
		<thead>
			<th>Assignee / Status</th>
			{{#each statuses as |status| }}
				<th>{{status}}</th>
			{{/each}}
			<th>Total</th>
		</thead>
		<tbody>
			{{#each perAssigneePerStatusIssueCount }}
				<tr>
					{{#each this}}
						{{#if @first}}
							<td><b>{{this}}</b></td>
						{{else if @last}}
							<td><b>{{this}}</b></td>
						{{else}}
							<td>{{this}}</td>
						{{/if}}
					{{/each}}
				</tr>
			{{/each}}
		</tbody>
	</table>

	<h2>Per assignee per status story point</h2>
	<table border="5">
		<thead>
			<th>Assignee / Status</th>
			{{#each statuses}}
				<th>{{this}}</th>
			{{/each}}
			<th>Total</th>
		</thead>
		<tbody>
			{{#each perAssigneePerStatusStoryPoint}}
				<tr>
					{{#each this}}
						{{#if @first}}
							<td><b>{{this}}</b></td>
						{{else if @last}}
							<td><b>{{this}}</b></td>
						{{else}}
							<td>{{this}}</td>
						{{/if}}
					{{/each}}
				</tr>
			{{/each}}
		</tbody>
	</table>

	<h2>Per tester issue "not in QA"</h2>
	<table border="5">
		<thead>
			<th>Tester</th>
			<th>Issue count</th>
		</thead>
		<tbody>
			{{#each perTestedByIssueNotInReadyToQA}}
				<tr>
					<td><b>{{@key}}</b></td>
					<td>{{this}}</td>
				</tr>
			{{/each}}
		</tbody>
	</table>

	<h2>"Ready to QA" & "Done" Statistics</h2>
	<table border="5">
		<thead>
			<th>Category / Status</th>
			<th>Backlog</th>
			<th>Ready to QA</th>
			<th>Done</th>
		</thead>
		<tbody>
				<tr>
					<td><b>Issue count</b></td>
					<td>{{issueCountAndStoryPointsInBacklog.issueCount}}</td>
					<td>{{issueCountAndStoryPointsInReadyToQA.issueCount}}</td>
					<td>{{issueCountAndStoryPointsInReadyToQA.issueCount}}</td>
				</tr>
				<tr>
					<td><b>Story points</b></td>
					<td>{{issueCountAndStoryPointsInBacklog.storyPoints}}</td>
					<td>{{issueCountAndStoryPointsInReadyToQA.storyPoints}}</td>
					<td>{{issueCountAndStoryPointsInReadyToQA.storyPoints}}</td>
				</tr>
		</tbody>
	</table>

</body>
</html>